import java.util.*;
public class practiceHemani {

	public static void main(String[] args) {
	
	printGrid(4,2);
	}

	public static void printGrid(int row, int column)
    {
        for (int i = 1; i <= row; i++)
        {
 
            for (int j = i; j <= row * column; j += 4)
            {
                System.out.print(j+" ");
            }
            System.out.println();
 
        }
 
    }


}